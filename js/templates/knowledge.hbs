<div class="area_contents">
	<div class="box_header none_border_bottom">
		<h2 class="contents_title">우수기술제품</h2>
	</div>
	<div class="box_contents none_top_bottom_padding">
		<div class="grid_inner">
			<!--  세로형 이미지 리스트 -->
			<ul class="wa_thumbnail_vert_list none_padding_top _btn_new_skill newSkill">
				{{#each newskill }}
					{{#isGt @index 3}}
						<li data-link="{{linkdetail}}" style="display:none">
							<a href="#">
								<div class="list_img">
									<img src="//www.water.or.kr/{{imgUrl}}" alt="">
								</div>
								<p class="list_title">{{title}}</p>
								<p class="list_txt">{{someHead}}</p>
							</a>
						</li>
					{{else}}
						<li data-link="{{linkdetail}}">
							<a href="#">
								<div class="list_img">
									<img src="//www.water.or.kr/{{imgUrl}}" alt="">
								</div>
								<p class="list_title">{{title}}</p>
								<p class="list_txt">{{someHead}}</p>
							</a>
						</li>
					{{/isGt}}
				{{/each}}
			</ul> <!-- //세로형 이미지 리스트 -->
		</div>

	</div>
    <a href="#" class="btn btn_wide_large _btn_newskill_more">더보기</a>
</div>
