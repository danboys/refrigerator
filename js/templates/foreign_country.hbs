
<!-- area_contents -->
<div class="area_contents">
	<div class="box_header">
		<h2 class="contents_title">글로벌트렌드 &amp; 프로젝트</h2>
		<a href="http://m.water.or.kr/overseas/globals/globals01.do" class="btn">더보기 <span class="sprite sprite_common right_sprite icon_more"></span></a>
	</div>
	<!-- box contents -->
	<div class="box_contents">
		<div class="horz_plus_text_list">
			<ul class="wa_thumbnail_horz_list">
				<li>
					<a href="http://m.water.or.kr/overseas/globals/globals01_detail.do?globaltrendSeqno={{trend.list.0.globaltrendSeqno }}" class="ui-link _btn_move_sub">
						<div class="list_img">
							<img src="{{removehttp trend.list.0.thumUrl}}" alt="">
						</div>
						<div class="list_info">
							<p class="list_title">{{trend.list.0.title}}</p>
							<ul class="list_txt">
								<li>등록일자 : {{trend.list.0.rgsdt}} </li>
								<li class="ellipsis01">기관 : {{trend.list.0.organ}}</li>
							</ul>
						</div>
					</a>
				</li>
				<li>
					<a href="http://m.water.or.kr/overseas/globals/globals01_detail.do?globaltrendSeqno={{trend.list.1.globaltrendSeqno }}" class="ui-link  _btn_move_sub">
						<div class="list_img">
							<img src="{{removehttp trend.list.1.thumUrl}}" alt="">
						</div>
						<div class="list_info">
							<p class="list_title">{{trend.list.1.title}}</p>
							<ul class="list_txt">
								<li>등록일자 : {{trend.list.1.rgsdt}} </li>
								<li class="ellipsis01">기관 :{{trend.list.1.organ}}</li>
							</ul>
						</div>
					</a>
				</li>
			</ul>
			<!-- 가로형 텍스트 리스트 -->
			<ul class="wa_text_list">
				{{#each trend.list}}
					{{#isRange @index 2 4}}
						<li>
							<a href="http://m.water.or.kr/overseas/globals/globals01_detail.do?globaltrendSeqno={{globaltrendSeqno}}" class="_btn_move_sub">
								<p class="list_title">{{title}}</p>
								<ul class="list_txt">
									<li class="date">{{rgsdt}}</li>
									<li class="by">{{organ}}</li>
								</ul>
							</a>
						</li>
					{{/isRange}}
				{{/each}}

			</ul> <!-- //가로형 텍스트 리스트 -->
		</div>
	</div> <!-- //box contents -->
</div>
<!-- //area_contents -->

