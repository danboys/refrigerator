<div class="area_contents">
	<div class="box_header none_border_bottom">
		<h2 class="contents_title">축제/행사</h2>
		<a href="http://m.water.or.kr/life/culture/culture01.do" class="btn">더보기
			<span class="sprite sprite_common right_sprite icon_more"></span></a>
	</div>
	<div class="box_contents none_top_bottom_padding">
		<div class="grid_inner">
			<!-- 상단 큰 이미지 가로형 이미지 리스트 ( 텍스트 영역 2줄 ) -->
			<ul class="wa_big_horz_list none_padding_top">
				{{#each festival.getJsonData}}
					<li>
						<a href="http://m.water.or.kr/life/culture/culture01_detail.do?contentid={{festivalCd}}" class="_btn_move_sub">
							<div class="list_img">
								{{#if imageUrlInfo}}
									<img src="{{removehttp imageUrlInfo}}" alt="">
								{{else}}
									<img src="//www.water.or.kr/upload/festival/thumb/{{referenceUrlInfo}}" alt="">
								{{/if}}
							</div>
							<div class="list_info">
								<p class="list_title">{{festivalNm}}</p>
								<ul class="list_txt">
									<li>기간 : {{festivalStartDt}} ~ {{festivalEndDt}}</li>
									<li>장소 : {{locationNm}}</li>
								</ul>
							</div>
						</a>
					</li>
				{{/each}}
			</ul> <!-- //상단 큰 이미지 가로형 이미지 리스트 ( 텍스트 영역 2줄 ) -->
