<div class="area_contents">
	<div class="box_header none_border_bottom">
		<h2 class="contents_title">생활상식</h2>
		<a href="/life/around/around01.do" class="btn _btn_move_sub">더보기
			<span class="sprite sprite_common right_sprite icon_more"></span></a>
	</div>
	<div class="box_contents none_top_bottom_padding">
		<!-- 상단 세로형, 하단 가로형 이미지 리스트 -->
		<div class="grid_inner thumb_plus_horz_list">
			<!-- 세로형 이미지 리스트 ( 텍스트 영역 2줄 ) -->
			<ul class="wa_thumbnail_vert_list none_padding_top">
				<!--어드민-->
				<li>
					<a href="http://m.water.or.kr/life/around/around01_detail.do?CONTS_ID={{sense.getJsonData.0.contsId}}" class="_btn_move_sub">
						<div class="list_img">
							<img src="//www.water.or.kr/{{sense.getJsonData.0.imgUrl}}" alt="">
						</div>
						<p class="list_title">{{sense.getJsonData.0.title}}</p>
						<p class="list_txt">{{sense.getJsonData.0.someHead}}</p>
					</a>
				</li>
				<li>
					<a href="http://m.water.or.kr/life/around/around01_detail.do?CONTS_ID={{sense.getJsonData.1.contsId}}" class="_btn_move_sub">
						<div class="list_img">
							<img src="//www.water.or.kr/{{sense.getJsonData.1.imgUrl}}" alt="">
						</div>
						<p class="list_title">{{sense.getJsonData.1.title}}</p>
						<p class="list_txt">{{sense.getJsonData.1.someHead}}</p>
					</a>
				</li>
			</ul> <!-- //세로형 이미지 리스트 ( 텍스트 영역 2줄 ) -->
			<!-- 가로형 이미지 리스트 ( 텍스트 영역 2줄 ) -->
			<ul class="wa_thumbnail_horz_list">
				<!--어드민데이터-->
				{{#each sense.getJsonData}}
					{{#isRange @index 2 5}}
						<li>
							<a href="http://m.water.or.kr/life/around/around01_detail.do?CONTS_ID={{contsId}}" class="_btn_move_sub">
								<div class="list_img">
									<img src="//www.water.or.kr/{{imgUrl}}" alt="">
								</div>
								<div class="list_info">
									<p class="list_title">{{title}}</p>
									<p class="list_txt">{{someHead}}</p>
